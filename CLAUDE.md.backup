def render_stl_with_pyvista(file_path, output_folder, total_images=360):
    # ä½¿ç”¨ PyVista é€²è¡Œé«˜å“è³ªæ¸²æŸ“
    # çƒé¢åº§æ¨™ç³»çµ±ç¢ºä¿è§’åº¦å‡å‹»åˆ†ä½ˆ
    # è‡ªå‹•æ¸…ç†èˆŠåœ–ç‰‡é¿å…é‡è¤‡

    images_per_elevation = 6  # å‚ç›´åˆ†å±¤
    images_per_azimuth = 60   # æ¯å±¤æ°´å¹³è§’åº¦æ•¸

    for elev_idx in range(images_per_elevation):
        elevation = -75 + (elev_idx * 150 / 5)  # -75Â° åˆ° +75Â°

        for az_idx in range(images_per_azimuth):
            azimuth = (az_idx * 360.0 / images_per_azimuth)  # 0Â° åˆ° 360Â°

            # çƒé¢åº§æ¨™è½‰ç¬›å¡çˆ¾åº§æ¨™
            x = distance * cos(elevation) * cos(azimuth)
            y = distance * cos(elevation) * sin(azimuth)
            z = distance * sin(elevation)

            # è¨­å®šç›¸æ©Ÿä½ç½®å’Œæ¸²æŸ“
            plotter.camera_position = [(x, y, z), (0, 0, 0), (0, 0, 1)]
